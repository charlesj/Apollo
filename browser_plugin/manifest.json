{
  "manifest_version": 2,

  "name": "Apollo",
  "description": "This extension for communicating with the Apollo server",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Bookmark"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
